# personal gitlab
Host gitlab.com
	HostName gitlab.com
	User git
	IdentityFile ~/.ssh/id_personal_gitlab
# personal github
Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_personal_github
	IdentitiesOnly yes
# work gitlab
Host gitlab.com-spotilla
	HostName gitlab.com
	User git
	IdentityFile ~/.ssh/id_work_gitlab
Host smapomon.com
  IdentityFile ~/.ssh/smapo_host_yubi_key
  Port 2169
Host ssh.smapomon.com
  IdentityFile ~/.ssh/smapo_host_yubi_key
  Port 2169
